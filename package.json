{
  "name": "voice-helper",
  "version": "0.0.3",
  "description": "Voice Helper for sites on TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "serve": "parcel serve index.html",
    "compile": "parcel build src/index.ts",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrsky1001/voice-helper.git"
  },
  "author": "mrsky1001",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrsky1001/voice-helper/issues"
  },
  "homepage": "https://github.com/mrsky1001/voice-helper#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{ts, tsx, js}": [
      "prettier --write --config .prettierr.yaml",
      "tslint --project tsconfig.json --fix --force",
      "sh lint.sh partner-account",
      "git add"
    ],
    "*.{html,scss,css}": [
      "prettier --write --config .prettierr.yaml",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "resizable": "^1.2.1",
    "sass-loader": "^10.0.1",
    "string-similarity": "^4.0.4",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jquery": "^3.5.5",
    "@types/node": "^12.7.12",
    "@types/parcel-env": "0.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-ts": "0.0.0",
    "codelyzer": "^6.0.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "node-sass": "^4.12.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.3",
    "parcel-proxy-server": "^1.1.0",
    "prettier": "^2.2.0",
    "sass": "^1.25.0",
    "sass-loader": "^7.1.0",
    "sys": "0.0.1",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tslint": "^6.1.3",
    "yandex-speller": "^4.1.0"
  }
}
